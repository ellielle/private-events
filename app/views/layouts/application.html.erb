<!DOCTYPE html>
<html lang="en">
  <%= render 'layouts/head' %>
  <body>
  <%= render 'layouts/header' %>
    <div class="container">
      <% flash.each do |message_type, msg| %>
        <%= content_tag :div, msg, class: "alert alert-#{message_type}" %>
      <% end %>
    <%= yield %>
    </div>
  </body>
</html>
