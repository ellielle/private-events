<% provide(:title, "Log in") %>
<h1>Log in</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: login_path) do |f| %>
      <%= f.label :username %>
      <%= f.text_field :username %>
      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>
    <%= link_to "New user? Sign up!", signup_url %>
  </div>
</div>