<% provide(:title, @event.title) %>
<%# TODO make page look better %>
<div class="attendees">
  <% if !@event.attendees.empty? %>
  <ul>
    <% @event.attendees.each do |attendee| %>
      <li><%= attendee.username %></li>
    <% end %>
  </ul>
  <% else %>
  <p>No one is attending this event.</p>
  <% end %>
</div>
